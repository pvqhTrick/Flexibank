{
  "name": "vanilla-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"{src,public}/**/*.{tsx,jsx,ts,js,less}\"",
    "lint": "eslint \"{src,public}/**/*.{tsx,jsx,ts,js}\" --fix",
    "update-all": "npx -p npm-check-updates ncu -u && npm install",
    "css:build": "lessc src/less.less src/less.css && tailwindcss src/less.css -o src/style.css --minify --watch"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-case-police": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^3.0.2",
    "less": "^4.2.2",
    "less-loader": "^12.2.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.8",
    "terser": "^5.39.0",
    "typescript": "5.7.3",
    "vite": "^6.1.1"
  },
  "dependencies": {
    "query-string": "^9.1.1"
  },
  "main": "eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pvqhTrick/Flexibank.git"
  },
